{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AppSettings": {
        "IsMessageTraceEnabled": false,
        "IsTimingTraceEnabled": false
    },
    "UserSettings": {
        "LogType": "Chainsaw",
        "PortChainsaw": 7071,
        "PortLogcat": 7081,
        "LogTimeFormat": "ConvertToLocalTime"
    }
}
